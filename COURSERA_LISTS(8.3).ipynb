    "numlist = list()\n",
    "while True:\n",
    "    inp = input(\"Enter the number :\")\n",
    "    if inp == 'done':\n",
    "        break\n",
    "    value = float(inp)\n",
    "    numlist.append(value)\n",
    "\n",
    "average = sum(numlist)/len(numlist)\n",
    "print(\"Average is :\",average)"
